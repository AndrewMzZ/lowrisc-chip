+
+#include <linux/kernel.h>
+#include <linux/fb.h>
+#include <linux/init.h>
+#include <linux/of_device.h>
+
+struct lowrisc_fb_info {
+	struct fb_info		*info;
+
+	char __iomem		*fb_base;
+	unsigned long		fb_base_phys;
+
+	struct device_node	*of_node;
+
+	unsigned int		width;
+	unsigned int		height;
+	unsigned int		depth;
+	unsigned int		fb_size;
+
+	u32			pseudo_palette_storage[16];
+};
+
+#define LOWRISC_REGS_PALETTE    32
+
+static int lowrisc_fb_get_props(struct lowrisc_fb_info *gp)
+{
+  gp->width = 640;
+  gp->height = 614;
+  gp->depth = 8;
+  return 0;
+}
+
+static void lowrisc_con_palette(int idx, int palette, struct fb_info *info)
+{
+  struct lowrisc_fb_info *gp = info->par;
+  uint64_t *hid_reg_ptr = (uint64_t *)(gp->fb_base + 0x4000);
+  if (hid_reg_ptr)
+    {
+      hid_reg_ptr[LOWRISC_REGS_PALETTE+(idx&15)] = palette;
+    }
+}
+
+
+static int lowrisc_fb_setcolreg(unsigned regno,
+			 unsigned blue, unsigned green, unsigned red,
+			 unsigned transp, struct fb_info *info)
+{
+  u32 value;
+        
+  pr_debug("Palette index %d set to %X,%X,%X\n", regno, red, green, blue);
+
+  if (regno < 16)
+    {
+      red >>= 8;
+      green >>= 8;
+      blue >>= 8;
+      
+      value = (red << 16) | (green << 8) | blue;
+      ((u32 *)(info->pseudo_palette))[regno] = value;
+      lowrisc_con_palette(regno, value, info);
+    }
+  
+  return 0;
+}
+
+static struct fb_ops lowrisc_fb_ops = {
+	.owner			= THIS_MODULE,
+	.fb_setcolreg		= lowrisc_fb_setcolreg,
+	.fb_fillrect		= cfb_fillrect,
+	.fb_copyarea		= cfb_copyarea,
+	.fb_imageblit		= cfb_imageblit,
+};
+
+static int lowrisc_fb_set_fbinfo(struct lowrisc_fb_info *gp)
+{
+	struct fb_info *info = gp->info;
+	struct fb_var_screeninfo *var = &info->var;
+
+	info->flags = FBINFO_DEFAULT;
+	info->fbops = &lowrisc_fb_ops;
+	info->screen_base = gp->fb_base + 0x48000;
+	info->screen_size = gp->fb_size - 0x48000;
+
+	info->pseudo_palette = gp->pseudo_palette_storage;
+
+	/* Fill fix common fields */
+	strlcpy(info->fix.id, "lowrisc_fb", sizeof(info->fix.id));
+        info->fix.smem_start = gp->fb_base_phys + 0x48000;
+        info->fix.smem_len = gp->fb_size - 0x48000;
+        info->fix.type = FB_TYPE_PACKED_PIXELS;
+	info->fix.visual = FB_VISUAL_PSEUDOCOLOR;
+
+	var->xres = gp->width;
+	var->yres = gp->height;
+	var->xres_virtual = var->xres;
+	var->yres_virtual = var->yres;
+	var->bits_per_pixel = gp->depth;
+
+	var->blue.offset = 0;
+	var->blue.length = 8;
+	var->green.offset = 8;
+	var->green.length = 8;
+	var->red.offset = 16;
+	var->red.length = 8;
+	var->transp.offset = 0;
+	var->transp.length = 0;
+
+	if (fb_alloc_cmap(&info->cmap, 16, 0)) {
+		printk(KERN_ERR "lowrisc_fb: Cannot allocate color map.\n");
+		return -ENOMEM;
+	}
+
+        return 0;
+}
+
+static int lowrisc_fb_probe(struct platform_device *ofdev)
+{
+	struct device_node *dp = ofdev->dev.of_node;
+	struct fb_info *info;
+	struct lowrisc_fb_info *gp;
+        struct resource *lowrisc_fb;
+	int err;
+
+        printk("lowrisc_fb_probe");
+        
+	info = framebuffer_alloc(sizeof(struct lowrisc_fb_info), &ofdev->dev);
+	if (!info) {
+		printk(KERN_ERR "lowrisc_fb: Cannot allocate fb_info\n");
+		err = -ENOMEM;
+		goto err_out;
+	}
+
+	gp = info->par;
+	gp->info = info;
+	gp->of_node = dp;
+
+        lowrisc_fb = platform_get_resource(ofdev, IORESOURCE_MEM, 0);
+        
+	gp->fb_base_phys = lowrisc_fb[0].start;
+
+	err = lowrisc_fb_get_props(gp);
+	if (err)
+		goto err_release_fb;
+
+	/* Framebuffer length is the same regardless of resolution. */
+	info->fix.line_length = 640;
+	gp->fb_size = lowrisc_fb[0].end - lowrisc_fb[0].start;
+
+	gp->fb_base = devm_ioremap_resource(&ofdev->dev, lowrisc_fb);
+
+	if (!gp->fb_base) {
+		err = -ENOMEM;
+		goto err_release_fb;
+	}
+
+	err = lowrisc_fb_set_fbinfo(gp);
+	if (err)
+		goto err_unmap_fb;
+
+	printk("lowrisc_fb: Found device at %pOF, mapped to %lX (size=%X)\n", dp, (size_t)(gp->fb_base), gp->fb_size);
+
+	err = register_framebuffer(info);
+	if (err < 0) {
+		printk(KERN_ERR "lowrisc_fb: Could not register framebuffer %pOF\n",
+		       dp);
+		goto err_unmap_fb;
+	}
+
+	dev_set_drvdata(&ofdev->dev, info);
+
+	return 0;
+
+err_unmap_fb:
+        iounmap(gp->fb_base);
+        
+err_release_fb:
+        framebuffer_release(info);
+
+err_out:
+	return err;
+}
+
+static const struct of_device_id lowrisc_fb_match[] = {
+	{
+		.name = "lowrisc-fb",
+	},
+	{},
+};
+
+static struct platform_driver lowrisc_fb_driver = {
+	.probe		= lowrisc_fb_probe,
+	.driver = {
+		.name			= "lowrisc-fb",
+		.of_match_table		= lowrisc_fb_match,
+		.suppress_bind_attrs	= true,
+	},
+};
+
+static int __init lowrisc_fb_init(void)
+{
+  printk("lowrisc_fb_init");
+  
+	return platform_driver_register(&lowrisc_fb_driver);
+}
+
+device_initcall(lowrisc_fb_init);
